{
    "name": "Bookmark Searcher",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Extension to search bookmarks",
    "icons": {
        "16": "icons/mg16x16.png",
        "32": "icons/mg32x32.png",
        "48": "icons/mg48x48.png",
        "128": "icons/mg128x128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "bookmarks"
    ]
}